# This file is for unifying the coding style for different editors and IDEs
# editorconfig.org

root = true

# Unix-style newlines with a newline ending every file
[*]
end_of_line = lf
charset = utf-8
indent_style = spaces
insert_final_newline = true

[*.rs]
indent_size = 4

[*.js]
indent_size = 2

[*.html]
indent_size = 2
